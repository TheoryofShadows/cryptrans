{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  },
  "include": [
    "api/**/*",
    "sdk/**/*",
    "tests/**/*"
  ],
  "exclude": [
    "node_modules",
    "programs",
    "circuits",
    "zkproof",
    "app"
  ]
}
