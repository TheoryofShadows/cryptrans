{
  "name": "cryptrans-circuits",
  "version": "0.2.0",
  "description": "Zero-knowledge circuits for CrypTrans anonymous voting",
  "scripts": {
    "compile": "circom vote.circom --r1cs --wasm --sym -o ../zkproof",
    "info": "snarkjs r1cs info ../zkproof/vote.r1cs",
    "powers": "node ../zkproof/download-ptau.js",
    "setup": "node ../zkproof/setup.js",
    "verify": "node ../zkproof/verify.js",
    "test": "node ../zkproof/test-circuit.js"
  },
  "dependencies": {
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "ffjavascript": "^0.3.0",
    "snarkjs": "^0.7.3"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "circom_tester": "^0.0.19"
  }
}
